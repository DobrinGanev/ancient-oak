{
  "name": "ancient-oak",
  "description": "Immutable data trees library",
  "keywords": [
    "immutable",
    "data structures",
    "data trees",
    "functional",
    "fp"
  ],
  "version": "0.3.5",
  "scripts": {
    "dist": "scripts/dist",
    "docs": "scripts/docs",
    "release": "scripts/release",
    "test": "JASMINE_CONFIG_PATH=jasmine.json jasmine",
    "watch-test": "NODE_ENV=test nodemon --watch lib --watch spec --exec npm test",
    "testv": "jasmine-node --verbose spec",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Szymon Witamborski <simon@brainshave.com>",
  "license": "MIT",
  "main": "./lib/index",
  "engines": {
    "node": ">= 0.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brainshave/ancient-oak.git"
  },
  "bugs": {
    "url": "https://github.com/brainshave/ancient-oak/issues"
  },
  "devDependencies": {
    "browserify": "3",
    "coveralls": "^2.11.6",
    "jasmine": "^2.4.1",
    "nodemon": "^1.9.0",
    "nyc": "^5.6.0",
    "uglify-js": "*",
    "underscore": "1"
  },
  "nyc": {
    "include": [
      "lib/**/*.js"
    ]
  }
}
